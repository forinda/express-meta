#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/orinda/Desktop/express-meta/node_modules/.pnpm/ttypescript@1.5.15_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.2__typescript@5.8.2/node_modules/ttypescript/bin/node_modules:/home/orinda/Desktop/express-meta/node_modules/.pnpm/ttypescript@1.5.15_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.2__typescript@5.8.2/node_modules/ttypescript/node_modules:/home/orinda/Desktop/express-meta/node_modules/.pnpm/ttypescript@1.5.15_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.2__typescript@5.8.2/node_modules:/home/orinda/Desktop/express-meta/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/orinda/Desktop/express-meta/node_modules/.pnpm/ttypescript@1.5.15_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.2__typescript@5.8.2/node_modules/ttypescript/bin/node_modules:/home/orinda/Desktop/express-meta/node_modules/.pnpm/ttypescript@1.5.15_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.2__typescript@5.8.2/node_modules/ttypescript/node_modules:/home/orinda/Desktop/express-meta/node_modules/.pnpm/ttypescript@1.5.15_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.2__typescript@5.8.2/node_modules:/home/orinda/Desktop/express-meta/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ttypescript@1.5.15_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.2__typescript@5.8.2/node_modules/ttypescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../.pnpm/ttypescript@1.5.15_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.2__typescript@5.8.2/node_modules/ttypescript/bin/tsserver" "$@"
fi
